{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:flowtype/recommended"
  ],
  "globals": {
    "test": true,
    "expect": true,
    "jest": true,
  },
  "rules": {
    "arrow-parens": [
      0
    ],
    "function-paren-newline": [
      0
    ],
    "no-confusing-arrow": [
      0
    ]
  },
  "plugins": [
    "flowtype"
  ]
}